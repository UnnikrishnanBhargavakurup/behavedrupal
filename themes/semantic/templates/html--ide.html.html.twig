  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/jquery.pwstrength.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/feedback.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/custom.js"></script>
  <script type="text/javascript" src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
  <script type="text/javascript" src="http://www.pureexample.com/js/lib/jquery.ui.touch-punch.min.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/dialogs.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/projects.js"></script>
  <script defer type="text/javascript" src="{{ base_path ~ directory }}/ide/js/mdl-selectfield.min.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/actions.js"></script>
  <script type="text/javascript" src="{{ base_path ~ directory }}/ide/js/jquery.auto-complete-custom.js"></script>
